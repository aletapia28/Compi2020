
 let   
    var j: Integer;    
    var i: Integer;    
    var k: Integer;    
    var num: Integer 
in
begin
    getint (var num); ! Number of iterations. 
    i:= 1;  
    j:= 2;
    while (num > 0) do  
    begin
        putint(i);
        put(' ');
        k:= i+j;
        i:= j;
        j:= k;
        num:= num - 1;  
    end;
end 