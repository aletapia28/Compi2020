let
  var i : Integer;
  var n : Integer;
  const m ~ 8;
  const d ~ 10
in
  i := 0;
  getint(var n);
  repeat loop ciclo do
    if i > n then
      exit 
    elsif i // d = 0 then
      put('d');
      puteol();
      putint(i);
      puteol();
      i := i + 1;
      next ciclo
    else
      nil
    end;
    i := i + m;
    putint(i);
    puteol()
  end
end
  
! OK